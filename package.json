{
  "name": "Vabble-DAO",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "hardhat clean",
    "compile": "npx hardhat compile",
    "test": "hardhat test test/main.test.js --network hardhat",
    "local:test": "hardhat test test/main.test.js --network localhost",
    "local:deploy": "hardhat --network localhost deploy",
    "test:mumbai": "hardhat test test/main.test.js --network mumbai",
    "fork:rinkeby": "hardhat node --fork https://eth-rinkeby.alchemyapi.io/v2/khT7j5E7O7LBI-Vf53jsKg9epwhAk2uh",
    "mainnet:deploy": "hardhat --network mainnet deploy",
    "mainnet:verify": "hardhat --network mainnet etherscan-verify --solc-input",
    "mainnet:export": "hardhat --network mainnet export --export exports/mainnet.json",
    "bscTest:deploy": "hardhat --network bscTest deploy",
    "bscTest:verify": "hardhat --network bscTest etherscan-verify --solc-input",
    "bscMain:deploy": "hardhat --network bscMain deploy",
    "bscMain:verify": "hardhat --network bscMain etherscan-verify --solc-input",
    "mumbai:deploy": "hardhat --network mumbai deploy",
    "mumbai:verify": "hardhat --network mumbai etherscan-verify --solc-input",
    "mumbai:export": "hardhat --network mumbai export --export exports/mumbai.json",
    "add:polygonAmoy": "hardhat run scripts/add_liquidity.js --network polygonAmoy",
    "pair:polygonAmoy": "hardhat run scripts/create_token_pair.js --network polygonAmoy",
    "polygonAmoy:deploy": "hardhat --network polygonAmoy deploy --tags Deploy",
    "polygonAmoy:verify": "hardhat --network polygonAmoy deploy --tags Verify",
    "polygonAmoy:export": "hardhat --network polygonAmoy export --export exports/polygonAmoy.json",
    "matic:deploy": "hardhat --network matic deploy",
    "matic:verify": "hardhat --network matic etherscan-verify --solc-input",
    "matic:export": "hardhat --network matic export --export exports/matic.json",
    "prettier": "prettier --write scripts/**/*.js && prettier --write test/**/*.js && prettier --write contracts/**/*.sol"
  },
  "keywords": [],
  "author": "Vabble",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-network-helpers": "^1.0.10",    
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^2.1.4",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "4.9.5",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "ethereum-waffle": "^4.0.10",
    "ethers": "^5.4.1",
    "hardhat": "^2.19.4",
    "hardhat-contract-sizer": "^2.0.3",
    "hardhat-deploy": "^0.11.45",
    "hardhat-deploy-ethers": "^0.3.0-beta.10",
    "hardhat-gas-reporter": "^1.0.8",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.16",
    "solidity-coverage": "^0.8.11",
    "@nomicfoundation/hardhat-verify": "^2.0.5"
  }
}
